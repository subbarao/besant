<%= div_for(tweet, :class => "bubble") do %>
  <%= image_tag tweet.mood, :class =>"mood" %>
  <div class="speech">
    <%= auto_link tweet.text, :urls, :target => :_blank %>
  </div>
  <%= image_tag tweet.profile_image_url, :width => 48, :height => 48, :rel => tweet.twitter_url, :class=>"avatar" %>
<% end %>
